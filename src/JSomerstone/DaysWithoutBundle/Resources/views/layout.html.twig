{% extends 'JSomerstoneDaysWithoutBundle::base.html.twig' %}
{% block head %}

    <link rel="stylesheet" href="{{ asset('bundles/jsomerstonedayswithout/css/dayswo.css') }}" type="text/css" media="all">

    <meta name="viewport" content="width=device-width, initial-scale=1">
{% endblock %}
{% block title %}Days w/o {{ title }}{% endblock %}
{% block content_header '' %}
{% block body %}
    <ul class="nav nav-tabs">
        <li {% if app.request.get('_route') == 'dwo_frontpage' %} class="active"{% endif %}>
            <a href="{{ url('dwo_frontpage') }}" >Home</a>
        </li>
        <li {% if app.request.get('_route') == 'dwo_loginpage' %} class="active"{% endif %}>
            <a href="{{ url('dwo_loginpage') }}" >Login</a>
        </li>
    </ul>

    <div class="container-fluid">
        {% include 'JSomerstoneDaysWithoutBundle::notifications.html.twig' %}
        {% block content '' %}
    </div>
{% endblock %}